<section class="allContacts">
	<h2><span>All Contacts</span></h2>
	<ul>
		<% $rolodex.contacts.each do |contact| %>
	 	<li>
	 		<a href="/contacts/<%= contact.id %>"><span>Name:</span> <%= contact.first_name %> <%= contact.last_name %> </a><span>Email:</span> <%= contact.email %> <span>Note:</span> <%= contact.note %>
	 	</li>
	 	<% end %>
		
	</ul>
</section>





