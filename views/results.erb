<section class="results">
<h2>Results</h2>
	<ul>
		<% @results.each do |contact| %>
	 	<li>
	 		<img class="get_started_icon" src="../User-Profile-128.png">
	 		<a href="/contacts/<%= contact.id %>"><span>Name:</span> <%= contact.first_name %> <%= contact.last_name %> </a></br><span>Email:</span> <%= contact.email %> </br><span>Note:</span> <%= contact.note %>
	 	</li>
	 	<% end %>

	 	<% if @results.blank? %>
	 	<p>No contacts match that search. <a href="/search">Try again?</a></p>
	 	<% end %>
	</ul>
</section>